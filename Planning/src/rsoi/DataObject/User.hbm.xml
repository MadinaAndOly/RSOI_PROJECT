<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="rsoi.DataObject">
	<class  name="User" table="user">
		<id name="id" column="id">
			<generator class="native"/>
		</id>
		<property name="guid" column="USER_GUID"/>
		<property name="login" column="USER_LOGIN"/>
		<property name="pwd" column="USER_PWD"/>
		<property name="name" column="USER_NAME"/>
		<property name="lname" column="USER_LNAME"/>
		<property name="mname" column="USER_MNAME"/>
		<property name="last_update" column="LAST_UPDATE_DT"/>

	</class>
</hibernate-mapping>